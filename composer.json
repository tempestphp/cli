{
    "name": "tempest/cli",
    "require": {
        "php": "^8.3",
        "tempest/console": "dev-main",
        "tempest/framework": "dev-main"
    },
    "minimum-stability": "dev",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tempest\\Cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\Cli\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aidan Casey",
            "email": "aidan@caseyhouse.net"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4@dev"
    }
}
